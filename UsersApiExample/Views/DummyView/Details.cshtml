@model UsersApiExample.Response.BasePaginationResponse<UsersApiExample.Response.UserDetailsResponse>

@{
        ViewData["Title"] = " Details";
  }

 

<!DOCTYPE html>
<html>
<head>
    <style>
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 100%;
}

td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;
}

tr:nth-child(even) {
  background-color: #dddddd;
}
</style>
</head>
<body>

<h2>HTML Table</h2>

    @if (Model is null)
    {
        <h1>None Found</h1>
    }
    else
    {
        <table>
            <tr>
                <th>ID</th>
                <th>Title</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Picture</th>
                <th>Gender</th>
                <th>Email</th>
                <th>Date Of Birth</th>
                <th>Phone</th>
                <th>Location</th>
                <th>Register Date</th>
                <th>Update Date</th>

               
        
       
            </tr>
            @foreach (var user in Model.Data)
            {
                <tr>
                    <td>@user.Id</td>
                    <td>@user.Title</td>
                    <td>@user.FirstName</td>
                    <td>@user.LastName</td>
                    <td><img src="@user.Picture" alt="picture"/></td>
                    <td>@user.Gender</td
                    <td>@user.Email</td
                    <td>@user.DateOfBirth</td>
                    <td>@user.Phone</td>
                    <td>@user.Location</td>
                    <td>@user.RegisterDate</td>
                    <td>@user.UpdatedDate</td>

                </tr>
            }

   
        </table>
    }
</body>
</html>